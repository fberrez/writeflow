{
  "name": "writeflow",
  "private": true,
  "scripts": {
 "build": "turbo build",
    "dev": "turbo dev",
    "start": "turbo start",
    "lint": "turbo lint",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "dev:backend": "pnpm dev --filter=@writeflow/backend",
    "dev:frontend": "pnpm dev --filter=@writeflow/frontend",
    "start:backend": "pnpm start --filter=@writeflow/backend",
    "start:frontend": "pnpm start --filter=@writeflow/frontend",
    "build:backend": "pnpm build --filter=@writeflow/backend",
    "build:frontend": "pnpm build --filter=@writeflow/frontend",
    "generate-api:frontend": "pnpm generate-api --filter=@writeflow/frontend"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
